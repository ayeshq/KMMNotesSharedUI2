-- Notes table
CREATE TABLE Note (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    title TEXT NOT NULL,
    content TEXT NOT NULL
);

-- Insert
INSERT INTO Note (title, content) VALUES (?, ?);

-- Update by id
UPDATE Note SET title = ?, content = ? WHERE id = ?;

-- Select all
SELECT * FROM Note;

-- Select by id
SELECT * FROM Note WHERE id = ?;

-- Delete by id
DELETE FROM Note WHERE id = ?;
